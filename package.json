{
  "name": "@griffonnage/sync",
  "version": "0.1.2",
  "description": "Synchronize drawings from Griffonnage",
  "author": {
    "name": "Romain Clement",
    "email": "contact@romain-clement.net",
    "url": "https://romain-clement.net"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/griffonnage/griffonnage-sync.git"
  },
  "bugs": {
    "url": "https://github.com/griffonnage/griffonnage-sync/issues"
  },
  "homepage": "https://github.com/griffonnage/griffonnage-sync#readme",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "tsc",
    "build": "npm run clean && npm run compile",
    "lint": "eslint --ext .ts,.js .",
    "lint:fix": "eslint --fix --ext .ts,.js .",
    "dev": "ts-node-dev --inspect --no-notify --respawn --transpileOnly ./src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "node-statsd": "^0.1.1",
    "pino-http": "^5.2.0",
    "socket.io": "^2.3.0",
    "socket.io-redis": "^5.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.22",
    "@types/node-statsd": "^0.1.2",
    "@types/pino-http": "^5.0.3",
    "@types/socket.io": "^2.1.8",
    "@types/socket.io-redis": "^1.0.26",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "prettier": "^2.0.4",
    "ts-jest": "^26.1.1",
    "ts-node-dev": "^1.0.0-pre.51",
    "typescript": "^3.9.6"
  },
  "engines": {
    "node": "^12.16.1",
    "npm": "^6.13.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
