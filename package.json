{
  "name": "@griffonnage/sync",
  "version": "0.1.0",
  "description": "Synchronize drawings from Griffonnage",
  "author": {
    "name": "Romain Clement",
    "email": "contact@romain-clement.net",
    "url": "https://romain-clement.net"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/griffonnage/griffonnage-sync.git"
  },
  "bugs": {
    "url": "https://github.com/griffonnage/griffonnage-sync/issues"
  },
  "homepage": "https://github.com/griffonnage/griffonnage-sync#readme",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "tsc",
    "build": "npm run clean && npm run compile",
    "lint": "eslint --ext .ts,.js .",
    "lint:fix": "eslint --fix --ext .ts,.js .",
    "dev": "ts-node-dev --inspect --no-notify --respawn --transpileOnly ./src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pino-http": "^5.1.0",
    "socket.io": "^2.3.0",
    "socket.io-redis": "^5.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/pino-http": "^5.0.1",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-redis": "^1.0.25",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.5.3",
    "prettier": "^2.0.4",
    "ts-jest": "^25.3.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": "^12.16.1",
    "npm": "^6.13.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
